{
  "name": "nira-react-ui",
  "version": "1.0.0",
  "description": "Libreria de Componentes UI para react",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"ui/**/*.{js,jsx,ts,tsx,json,css}\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "check": "npm run format && npm run lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build:css": "postcss ui/styles/globals.scss -o dist/index.css",
    "build": "vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nira-software/nira-react-ui.git"
  },
  "author": {
